<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Shaman.ValueString</id>
    <version>1.0.2.31</version>
    <authors>Andrea Martinelli</authors>
    <owners>Andrea Martinelli</owners>
